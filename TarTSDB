USE [master]
GO

CREATE DATABASE [TarTS]
GO

Use TarTS
Go

CREATE TABLE [dbo].[AddressType](
    [AddressTypeID] [int] NOT NULL,
    [AddressTypeDescription] [varchar](20) NULL,
 CONSTRAINT [AddressType_PK] PRIMARY KEY CLUSTERED
(
    [AddressTypeID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO

CREATE TABLE [dbo].[CitizenData](
    [SSN] [varchar](11) NULL,
    [TaxID] [int] IDENTITY(1000,1)NOT NULL,
    [Salutation] [varchar](4) NOT NULL,
    [Suffix] [nvarchar](4) NULL,
    [LastName] [varchar](80) NOT NULL,
    [FirstName] [varchar](80) NOT NULL,
    [MiddleName] [varchar](80) NULL,
    [BirthDate] [datetime] NOT NULL,
    [ContactID] [int] NULL,
    [EmployerID] [int] NULL,
 CONSTRAINT [CitizenData_PK] PRIMARY KEY CLUSTERED
(
    [TaxID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO
SET ANSI_PADDING OFF
GO

CREATE TABLE [dbo].[ContactInformation](
    [ContactID] [int] IDENTITY(1000,1) NOT NULL,
    [PhoneTypeID] [int] NULL,
    [AddressTypeID] [int] NULL,
    [Address1] [varchar](80) NULL,
    [Address2] [varchar](80) NULL,
    [City] [varchar](80) NULL,
    [State] [varchar](2) NULL,
    [ZIP] [varchar](10) NULL,
 CONSTRAINT [ContactInformationData_PK] PRIMARY KEY CLUSTERED
(
    [ContactID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO
SET ANSI_PADDING OFF
GO

CREATE TABLE [dbo].[EmployerData](
    [EmployerID] [int] IDENTITY(1000,1) NOT NULL,
    [TaxID] [int] NOT NULL,
    [EmployerName] [varchar](80) NULL,
    [ContactID] [int] NULL,
 CONSTRAINT [EmployerData_PK] PRIMARY KEY CLUSTERED
(
    [TaxID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO
SET ANSI_PADDING OFF
GO

CREATE TABLE [dbo].[PhoneInformation](
    [PhoneID] [int] IDENTITY(1000,1) NOT NULL,
    [PhoneNum] [varchar](10) NULL,
    [PhoneTypeID] [int] NULL
) ON [PRIMARY]

CREATE TABLE [dbo].[PhoneType](
    [PhoneTypeID] [int] IDENTITY(1,1) NOT NULL,
    [PhoneType] [varchar](10)  NULL
) ON [PRIMARY]

CREATE TABLE [dbo].[Resolution](
    [TaxID] [int] NOT NULL,
    [ResolutionID] [int] IDENTITY(1000,1) NOT NULL,
    [HasFiled] [bit] NOT NULL,
    [PaymentID] [int] NULL,
    [DateResolved] [datetime] NOT NULL,
    [ResolutionDescription] [varchar](500) NULL,
 CONSTRAINT [Resolution_PK] PRIMARY KEY CLUSTERED
(
    [ResolutionID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO
SET ANSI_PADDING OFF
GO

CREATE TABLE [dbo].[ReturnResolution](
    [ReturnID] [int] NOT NULL,
    [ResolutionID] [int] NOT NULL,
 CONSTRAINT [ReturnResolution_PK] PRIMARY KEY CLUSTERED
(
    [ReturnID] ASC,
    [ResolutionID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO

CREATE TABLE [dbo].[TaxReturnInformation](
    [TaxID] [int] NOT NULL,
    [PreparerID] [int] NULL,
    [ReturnID] [int] IDENTITY(1000,1) NOT NULL,
    [ReturnType] [varchar](10) NOT NULL,
    [FilingStatus] [varchar](10) NOT NULL,
    [ReturnAmount] [money] NULL,
    [DateFiled] [datetime] NULL,
    [DateDispersed] [datetime] NULL,
 CONSTRAINT [TaxReturnInformation_PK] PRIMARY KEY CLUSTERED
(
    [ReturnID] ASC
)WITH (PAD_INDEX  = OFF, IGNORE_DUP_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]

GO
SET ANSI_PADDING OFF
GO

ALTER TABLE [dbo].[CitizenData]  WITH CHECK ADD  CONSTRAINT [Citizen_Contact_FK] FOREIGN KEY([ContactID])
REFERENCES [dbo].[ContactInformation] ([ContactID])
GO
ALTER TABLE [dbo].[CitizenData] CHECK CONSTRAINT [Citizen_Contact_FK]

ALTER TABLE [dbo].[ContactInformation]  WITH CHECK ADD  CONSTRAINT [Address_Contact_FK] FOREIGN KEY([AddressTypeID])
REFERENCES [dbo].[AddressType] ([AddressTypeID])
GO
ALTER TABLE [dbo].[ContactInformation] CHECK CONSTRAINT [Address_Contact_FK]

ALTER TABLE [dbo].[EmployerData]  WITH CHECK ADD  CONSTRAINT [Citizen_Employer_FK] FOREIGN KEY([TaxID])
REFERENCES [dbo].[CitizenData] ([TaxID])
GO
ALTER TABLE [dbo].[EmployerData] CHECK CONSTRAINT [Citizen_Employer_FK]
GO
ALTER TABLE [dbo].[EmployerData]  WITH CHECK ADD  CONSTRAINT [Contact_Emplployer_FK] FOREIGN KEY([ContactID])
REFERENCES [dbo].[ContactInformation] ([ContactID])
GO
ALTER TABLE [dbo].[EmployerData] CHECK CONSTRAINT [Contact_Emplployer_FK]

ALTER TABLE [dbo].[Resolution]  WITH CHECK ADD  CONSTRAINT [Citizen_Resolution_ResolutionID_FK] FOREIGN KEY([ResolutionID])
REFERENCES [dbo].[CitizenData] ([TaxID])
GO
ALTER TABLE [dbo].[Resolution] CHECK CONSTRAINT [Citizen_Resolution_ResolutionID_FK]

ALTER TABLE [dbo].[ReturnResolution]  WITH CHECK ADD  CONSTRAINT [ReturnResolution_Resolution_FK] FOREIGN KEY([ResolutionID])
REFERENCES [dbo].[Resolution] ([ResolutionID])
GO
ALTER TABLE [dbo].[ReturnResolution] CHECK CONSTRAINT [ReturnResolution_Resolution_FK]
GO
ALTER TABLE [dbo].[ReturnResolution]  WITH CHECK ADD  CONSTRAINT [ReturnResolution_TaxReturnInformation_FK] FOREIGN KEY([ReturnID])
REFERENCES [dbo].[TaxReturnInformation] ([ReturnID])
GO
ALTER TABLE [dbo].[ReturnResolution] CHECK CONSTRAINT [ReturnResolution_TaxReturnInformation_FK]

ALTER TABLE [dbo].[TaxReturnInformation]  WITH CHECK ADD  CONSTRAINT [Citizen_Preparer_FK] FOREIGN KEY([PreparerID])
REFERENCES [dbo].[CitizenData] ([TaxID])
GO
ALTER TABLE [dbo].[TaxReturnInformation] CHECK CONSTRAINT [Citizen_Preparer_FK]
GO
ALTER TABLE [dbo].[TaxReturnInformation]  WITH CHECK ADD  CONSTRAINT [Citizen_TaxReturn_TaxIDNu_FK] FOREIGN KEY([TaxID])
REFERENCES [dbo].[CitizenData] ([TaxID])
GO
ALTER TABLE [dbo].[TaxReturnInformation] CHECK CONSTRAINT [Citizen_TaxReturn_TaxIDNu_FK]
